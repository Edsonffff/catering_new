{
  "name": "catering-website",
  "version": "1.0.0",
  "description": "Full-stack catering website with admin dashboard",
  "main": "server/server.js",
  "scripts": {
    "server": "node server/server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --prefix client",
    "install-all": "npm install && cd client && npm install",
    "setup": "npm install && cd client && npm install && cd .. && node server/config/initDatabase.js"
  },
  "keywords": [
    "catering",
    "restaurant",
    "orders",
    "admin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.16.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "type": "commonjs"
}
